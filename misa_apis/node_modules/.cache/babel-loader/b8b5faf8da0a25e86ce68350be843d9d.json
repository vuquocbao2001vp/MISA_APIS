{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  props: [\"isHideDialogImport\"],\n\n  data() {\n    return {\n      inputFile: null,\n      totalValid: null,\n      totalInvalid: null,\n      validUsers: [],\n      invalidUsers: []\n    };\n  },\n\n  methods: {\n    /**\r\n     * Đóng dialog nhập khẩu\r\n     * Author: VQBao - 20/9/2022\r\n     */\n    closeDialog() {\n      this.$emit(\"hideDialogImport\", true);\n      this.$refs.fileImport.value = null;\n      this.totalValid = null;\n      this.totalInvalid = null;\n      this.validUsers = [];\n      this.invalidUsers = [];\n    },\n\n    /**\r\n     * Khi click vào button nhập khẩu, tạo sự kiện click chọn file của input\r\n     */\n    importFileOnClick() {\n      this.$refs.fileImport.click();\n    },\n\n    /**\r\n     * Khi chọn xong file thì thực hiện gọi api lấy dữ liệu số bản ghi hợp lệ, bản ghi lỗi, danh sách bản ghi hợp lệ, danh sách bản ghi lỗi\r\n     * Author: VQBao - 20/9/2022\r\n     */\n    changeFile() {\n      var formImport = new FormData();\n      this.inputFile = document.getElementById(\"fileImport\").files[0];\n      formImport.append(\"fileImport\", this.inputFile);\n\n      if (this.inputFile != null) {\n        try {\n          axios.post(\"https://localhost:44382/api/v1/Users/GetImportQuantity\", formImport, {\n            headers: {\n              \"Content-Type\": \"multipart/form-data\"\n            }\n          }).then(response => {\n            this.totalValid = response.data.TotalValid;\n            this.totalInvalid = response.data.TotalInvalid;\n            this.validUsers = response.data.ValidUsers;\n            this.invalidUsers = response.data.InvalidUsers;\n          });\n        } catch (error) {\n          console.log(error);\n        }\n      }\n    },\n\n    /**\r\n     * Khi click vào tải file mẫu, gọi api tải về file excel nhập liệu mẫu\r\n     * Author: VQBao - 20/9/2022\r\n     */\n    exportSampleExcel() {\n      document.getElementById(\"loader\").style;\n\n      try {\n        axios({\n          url: \"https://localhost:44382/api/v1/Users/ExportSampleExcel\",\n          method: \"GET\",\n          responseType: \"blob\"\n        }).then(response => {\n          var fileURL = window.URL.createObjectURL(new Blob([response.data]));\n          var fileLink = document.createElement(\"a\");\n          fileLink.href = fileURL;\n          fileLink.setAttribute(\"download\", \"Danh_sach_mau.xlsx\");\n          document.body.appendChild(fileLink);\n          fileLink.click();\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Khi click chọn nhập khẩu, gọi api nhập khẩu những bản ghi hợp lệ\r\n     * Author: VQBao - 20/9/2022\r\n     */\n    importValidUser() {\n      if (this.validUsers.length > 0) {\n        try {\n          axios.post(\"https://localhost:44382/api/v1/Users/Import\", this.validUsers, {\n            headers: {\n              \"Content-Type\": \"application/json\"\n            }\n          }).then(() => {\n            this.validUsers = [];\n            console.log(\"ok\");\n          });\n        } catch (error) {\n          console.log(error);\n        }\n      }\n    },\n\n    /**\r\n     * Xuất khẩu ra file excel những bản ghi bị lỗi\r\n     * Author: VQBao - 20/9/2022\r\n     */\n    exportInvalidUser() {\n      if (this.invalidUsers.length > 0) {\n        try {\n          axios({\n            url: \"https://localhost:44382/api/v1/Users/ExportInvalidUser\",\n            method: \"POST\",\n            data: this.invalidUsers,\n            responseType: \"blob\"\n          }).then(response => {\n            var fileURL = window.URL.createObjectURL(new Blob([response.data]));\n            var fileLink = document.createElement(\"a\");\n            fileLink.href = fileURL;\n            fileLink.setAttribute(\"download\", \"Danh_sach_nguoi_dung_loi.xlsx\");\n            document.body.appendChild(fileLink);\n            fileLink.click();\n            this.invalidUsers = [];\n          });\n        } catch (error) {\n          console.log(error);\n        }\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,KAAK,EAAE,CAAC,oBAAD,CADM;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,IADN;MAELC,UAAU,EAAE,IAFP;MAGLC,YAAY,EAAE,IAHT;MAILC,UAAU,EAAE,EAJP;MAKLC,YAAY,EAAE;IALT,CAAP;EAOD,CAVY;;EAWbC,OAAO,EAAE;IACP;;;;IAIAC,WAAW,GAAG;MACZ,KAAKC,KAAL,CAAW,kBAAX,EAA+B,IAA/B;MACA,KAAKC,KAAL,CAAWC,UAAX,CAAsBC,KAAtB,GAA8B,IAA9B;MACA,KAAKT,UAAL,GAAkB,IAAlB;MACA,KAAKC,YAAL,GAAoB,IAApB;MACA,KAAKC,UAAL,GAAkB,EAAlB;MACA,KAAKC,YAAL,GAAoB,EAApB;IACD,CAZM;;IAcP;;;IAGAO,iBAAiB,GAAG;MAClB,KAAKH,KAAL,CAAWC,UAAX,CAAsBG,KAAtB;IACD,CAnBM;;IAqBP;;;;IAIAC,UAAU,GAAG;MACX,IAAIC,UAAS,GAAI,IAAIC,QAAJ,EAAjB;MACA,KAAKf,SAAL,GAAiBgB,QAAQ,CAACC,cAAT,CAAwB,YAAxB,EAAsCC,KAAtC,CAA4C,CAA5C,CAAjB;MACAJ,UAAU,CAACK,MAAX,CAAkB,YAAlB,EAAgC,KAAKnB,SAArC;;MACA,IAAI,KAAKA,SAAL,IAAkB,IAAtB,EAA4B;QAC1B,IAAI;UACFH,KAAI,CACDuB,IADH,CAEI,wDAFJ,EAGIN,UAHJ,EAII;YACEO,OAAO,EAAE;cACP,gBAAgB;YADT;UADX,CAJJ,EAUGC,IAVH,CAUSC,QAAD,IAAc;YAClB,KAAKtB,UAAL,GAAkBsB,QAAQ,CAACxB,IAAT,CAAcyB,UAAhC;YACA,KAAKtB,YAAL,GAAoBqB,QAAQ,CAACxB,IAAT,CAAc0B,YAAlC;YACA,KAAKtB,UAAL,GAAkBoB,QAAQ,CAACxB,IAAT,CAAc2B,UAAhC;YACA,KAAKtB,YAAL,GAAoBmB,QAAQ,CAACxB,IAAT,CAAc4B,YAAlC;UACD,CAfH;QAgBF,CAjBA,CAiBE,OAAOC,KAAP,EAAc;UACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;QACF;MACF;IACD,CAnDM;;IAqDP;;;;IAIAG,iBAAiB,GAAG;MAClBf,QAAQ,CAACC,cAAT,CAAwB,QAAxB,EAAkCe,KAAlC;;MACA,IAAI;QACFnC,KAAK,CAAC;UACJoC,GAAG,EAAE,wDADD;UAEJC,MAAM,EAAE,KAFJ;UAGJC,YAAY,EAAE;QAHV,CAAD,CAAL,CAIGb,IAJH,CAISC,QAAD,IAAc;UACpB,IAAIa,OAAM,GAAIC,MAAM,CAACC,GAAP,CAAWC,eAAX,CAA2B,IAAIC,IAAJ,CAAS,CAACjB,QAAQ,CAACxB,IAAV,CAAT,CAA3B,CAAd;UACA,IAAI0C,QAAO,GAAIzB,QAAQ,CAAC0B,aAAT,CAAuB,GAAvB,CAAf;UAEAD,QAAQ,CAACE,IAAT,GAAgBP,OAAhB;UACAK,QAAQ,CAACG,YAAT,CAAsB,UAAtB,EAAkC,oBAAlC;UACA5B,QAAQ,CAAC6B,IAAT,CAAcC,WAAd,CAA0BL,QAA1B;UACAA,QAAQ,CAAC7B,KAAT;QACD,CAZD;MAaF,CAdA,CAcE,OAAOgB,KAAP,EAAc;QACdC,OAAO,CAACC,G