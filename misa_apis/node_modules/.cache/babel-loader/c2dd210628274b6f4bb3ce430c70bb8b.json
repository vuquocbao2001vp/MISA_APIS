{"ast":null,"code":"import DxSelectBox from \"devextreme-vue/select-box\";\nexport default {\n  components: {\n    DxSelectBox\n  },\n  props: ['paging'],\n\n  data() {\n    return {\n      pageSelection: [15, 25, 50, 100],\n      dPageSize: 50\n    };\n  },\n\n  watch: {\n    pageSize: function (value) {\n      this.dPageSize = value;\n    },\n    dPageSize: function (value) {\n      this.$emit(\"getData\", {\n        pageSize: value,\n        pageNumber: 1,\n        searchKey: this.filterText\n      });\n    }\n  },\n  methods: {\n    /**\r\n     * Chuyển về trang trước đó\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    toPrevPage() {\n      if (this.pageNumber > 1) {\n        this.$emit(\"getData\", {\n          pageSize: this.dPageSize,\n          pageNumber: this.pageNumber - 1,\n          searchKey: this.filterText\n        });\n      }\n    },\n\n    /**\r\n     * Chuyển đến trang tiếp theo\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    toNextPage() {\n      if (this.pageNumber < this.totalPages) {\n        this.$emit(\"getData\", {\n          pageSize: this.dPageSize,\n          pageNumber: this.pageNumber + 1,\n          searchKey: this.filterText\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,WAAP,MAAwB,2BAAxB;AACA,eAAe;EACbC,UAAU,EAAE;IAACD;EAAD,CADC;EAEbE,KAAK,EAAE,CAAC,QAAD,CAFM;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,CAAC,EAAD,EAAK,EAAL,EAAS,EAAT,EAAa,GAAb,CADV;MAELC,SAAS,EAAE;IAFN,CAAP;EAID,CARY;;EASbC,KAAK,EAAE;IACLC,QAAQ,EAAE,UAASC,KAAT,EAAe;MACvB,KAAKH,SAAL,GAAiBG,KAAjB;IACD,CAHI;IAILH,SAAS,EAAE,UAASG,KAAT,EAAe;MACxB,KAAKC,KAAL,CAAW,SAAX,EAAsB;QAACF,QAAQ,EAAEC,KAAX;QAAkBE,UAAU,EAAE,CAA9B;QAAiCC,SAAS,EAAE,KAAKC;MAAjD,CAAtB;IACF;EANK,CATM;EAiBbC,OAAO,EAAE;IACP;;;;IAIAC,UAAU,GAAE;MACV,IAAG,KAAKJ,UAAL,GAAkB,CAArB,EAAuB;QACrB,KAAKD,KAAL,CAAW,SAAX,EAAsB;UAACF,QAAQ,EAAE,KAAKF,SAAhB;UAA2BK,UAAU,EAAE,KAAKA,UAAL,GAAgB,CAAvD;UAA0DC,SAAS,EAAE,KAAKC;QAA1E,CAAtB;MACF;IACD,CATM;;IAUP;;;;IAIAG,UAAU,GAAE;MACV,IAAG,KAAK