);\n      var self = this;\n      this.timeout = setTimeout(function () {\n        self.textSearch = data.value;\n      }, 500);\n    },\n\n    /**\r\n     * Khi chọn nhóm người dùng, phân trang người dùng lọc theo nhóm người dùng đó\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    getGroupItemSearch(data) {\n      this.groupItemSearch = data.value;\n\n      try {\n        this.getUserData({\n          pageSize: this.userPaging.pageSize,\n          pageNumber: 1,\n          searchByText: this.textSearch,\n          searchByGroup: data.value\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Khi click vào hàng trong bảng thì cửa sổ thông tin chi tiết hiện ra\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    rowOnClick({\n      selectedRowsData\n    }) {\n      console.log(selectedRowsData[0]);\n      this.userSelectedId = selectedRowsData[0].UserId;\n      this.getUserDataById(this.userSelectedId);\n      this.showDialog(true);\n    },\n\n    /**\r\n     * Ẩn hiện cửa sổ thông tin chi tiết người dùng\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    showDialog(isShow) {\n      this.isShowDialog = isShow;\n    },\n\n    deleteUser() {\n      alert(\"ok\");\n    },\n\n    /**\r\n     * Ẩn hiện cửa sổ popup tùy chỉnh cột\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    reorderColumn() {\n      this.isHideReorderPopup = !this.isHideReorderPopup;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmGA,OAAOA,gBAAP,MAA6B,wBAA7B;AACA,SAAQC,YAAR,EAAsBC,UAAtB,EAAkCC,UAAlC,QAAmD,MAAnD;AACA,eAAe;EACbC,UAAU,EAAE;IACVJ;EADU,CADC;;EAIbK,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,KADT;MAELC,kBAAkB,EAAE,IAFf;MAGLC,UAAU,EAAE,EAHP;MAILC,eAAe,EAAE,EAJZ;MAKLC,OAAO,EAAE,IALJ;MAMLC,cAAc,EAAE,IANX;MAQLC,UAAU,EAAE,CAAC,WAAD,EAAc,SAAd,EAAyB,WAAzB,CARP;MASLC,MAAM,EAAC,CAAC,YAAD,EAAe,sBAAf,EAAuC,uBAAvC,EAAgE,YAAhE,EAA8E,QAA9E;IATF,CAAP;EAWD,CAhBY;;EAiBbC,QAAQ,EAAE,EACR,GAAGX,UAAU,CAAC,CAAC,OAAD,EAAU,YAAV,EAAwB,cAAxB,EAAwC,qBAAxC,CAAD;EADL,