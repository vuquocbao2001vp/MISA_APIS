{"ast":null,"code":"import DialogUserDetail from \"./DialogUserDetail.vue\";\nimport { mapMutations, mapActions, mapGetters } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    DialogUserDetail\n  },\n\n  data() {\n    return {\n      isShowDialog: false,\n      isHideReorderPopup: true,\n      isHidePopup: true,\n      textSearch: \"\",\n      groupItemSearch: \"\",\n      timeout: null,\n      userSelectedId: null,\n      userIdToDelete: null,\n      isHideToast: true,\n      groups: [],\n      coloumName: [\"Họ và tên\", \"Chức vụ\", \"Phòng ban\", \"Đơn vị\", \"Email\", \"Nhóm người dùng\", \"Điện thoại di động\", \"Điện thoại cơ quan\", \"Trạng thái\"]\n    };\n  },\n\n  computed: { ...mapGetters([\"users\", \"userPaging\", \"userGroups\"])\n  },\n\n  mounted() {\n    this.getUserData({\n      pageSize: 50,\n      pageNumber: 1,\n      searchByText: \"\",\n      searchByGroup: \"\"\n    });\n    this.getAllGroupData(); // this.userGroups.unshift({'UserGroupID': 100, 'UserGroupName': \"Tất cả\"})\n    // this.groups = this.userGroups.unshift({UserGroupID: 100, UserGroupName: \"Tất cả\"});\n    // console.log(this.groups);\n  },\n\n  watch: {\n    /**\r\n     * Khi từ khóa tìm kiếm thay đổi, load lại paging\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    textSearch: function (value) {\n      try {\n        this.getUserData({\n          pageSize: this.userPaging.pageSize,\n          pageNumber: 1,\n          searchByText: value,\n          searchByGroup: this.groupItemSearch\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  },\n  methods: { ...mapMutations([\"getUserPaging\", \"getUserById\", \"getAllGroup\"]),\n    ...mapActions([\"getUserData\", \"getUserDataById\", \"getAllGroupData\"]),\n\n    /**\r\n     * Phương thức thực hiện phân trang người dùng\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    loadUserPaging(pageSize, pageNumber) {\n      try {\n        this.getUserData({\n          pageSize: pageSize,\n          pageNumber: pageNumber,\n          searchByText: this.textSearch,\n          searchByGroup: this.groupItemSearch\n        });\n        this.userSelectedId = null;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Load lại data đầy đủ thông tin paging khi có thay đổi về dữ liệu\r\n     * Author: VQBao - 10/8/2022\r\n     */\n    loadData() {\n      try {\n        this.getUserData({\n          pageSize: this.userPaging.pageSize,\n          pageNumber: this.userPaging.pageNumber,\n          searchByText: this.textSearch,\n          searchByGroup: this.groupItemSearch\n        });\n        this.userSelectedId = null;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Lấy ra từ khóa tìm kiếm sau khi nhập xong input\r\n     * Author; VQBao - 8/9/2022\r\n     */\n    getTextSearch(data) {\n      clearTimeout(this.timeout);\n      var self = this;\n      this.timeout = setTimeout(function () {\n        self.textSearch = data.value;\n      }, 500);\n    },\n\n    /**\r\n     * Khi chọn nhóm người dùng, phân trang người dùng lọc theo nhóm người dùng đó\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    getGroupItemSearch(data) {\n      this.groupItemSearch = data.value;\n\n      try {\n        this.getUserData({\n          pageSize: this.userPaging.pageSize,\n          pageNumber: 1,\n          searchByText: this.textSearch,\n          searchByGroup: data.value\n        });\n        this.userSelectedId = null;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Khi click vào hàng trong bảng thì cửa sổ thông tin chi tiết hiện ra\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    rowOnClick(userRow) {\n      this.userSelectedId = userRow.data.UserID;\n      this.showDialog(true);\n    },\n\n    /**\r\n     * Ẩn hiện cửa sổ thông tin chi tiết người dùng\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    showDialog(isShow) {\n      this.isShowDialog = isShow;\n    },\n\n    /**\r\n     * Ẩn hiện popup thông báo\r\n     * Author: VQBao - 10/8/2022\r\n     */\n    hidePopup(isHide) {\n      this.isHidePopup = isHide;\n    },\n\n    /**\r\n     * Hiện toast message\r\n     * Author: VQBao - 10/8/2022\r\n     */\n    hideToast() {\n      var me = this;\n      me.isHideToast = false;\n      setTimeout(function () {\n        me.isHideToast = true;\n      }, 3000);\n    },\n\n    /**\r\n     * Xóa người dùng khi bấm vào button xóa ở cuối dòng trong bảng\r\n     * Author: VQBao - 10/9/2022\r\n     */\n    deleteUserOnClick(id) {\n      this.userIdToDelete = id;\n      this.hidePopup(false);\n    },\n\n    /**\r\n     * Xóa người dùng(dùng để emit sau confirm xóa)\r\n     * Author: VQBao - 10/9/2022\r\n     */\n    deleteUser() {\n      try {\n        axios.delete(`https://localhost:44382/api/v1/Users/` + this.userIdToDelete).then(() => {\n          // load lại data\n          this.getUserData({\n            pageSize: this.userPaging.pageSize,\n            pageNumber: 1,\n            searchByText: this.textSearch,\n            searchByGroup: this.groupItemSearch\n          });\n          this.hideToast();\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Ẩn hiện cửa sổ popup tùy chỉnh cột\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    reorderColumn() {\n      this.isHideReorderPopup = !this.isHideReorderPopup;\n    },\n\n    /**\r\n     * Xuất khẩu toàn bộ người dùng ra file excel\r\n     * Author: VQBao - 12/9/2022\r\n     */\n    exportToExcel() {\n      try {\n        axios({\n          url: \"https://localhost:44382/api/v1/Users/ExportToExcel\",\n          method: \"GET\",\n          responseType: \"blob\"\n        }).then(response => {\n          var fileURL = window.URL.createObjectURL(new Blob([response.data]));\n          var fileLink = document.createElement(\"a\");\n          fileLink.href = fileURL;\n          fileLink.setAttribute(\"download\", \"Danh_sach_nguoi_dung.xlsx\");\n          document.body.appendChild(fileLink);\n          fileLink.click();\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyJA,OAAOA,gBAAP,MAA6B,wBAA7B;AACA,SAASC,YAAT,EAAuBC,UAAvB,EAAmCC,UAAnC,QAAqD,MAArD;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAE;IACVL;EADU,CADC;;EAIbM,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,KADT;MAELC,kBAAkB,EAAE,IAFf;MAGLC,WAAW,EAAE,IAHR;MAILC,UAAU,EAAE,EAJP;MAKLC,eAAe,EAAE,EALZ;MAMLC,OAAO,EAAE,IANJ;MAOLC,cAAc,EAAE,IAPX;MAQLC,cAAc,EAAE,IARX;MASLC,WAAW,EAAE,IATR;MAULC,MAAM,EAAE,EAVH;MAYLC,UAAU,EAAE,CACV,WADU,EAEV,SAFU,EAGV,WAHU,EAIV,QAJU,EAKV,OALU,EAMV,iBANU,EAOV,oBAPU,EAQV,oBARU,EASV,YATU;IAZP,CAAP;EAwBD,CA7BY;;EA8BbC,QAAQ,EAAE,EACR,GAAGf,UAAU,CAAC,CAAC,OAAD,EAAU,YAAV,EAAwB,YAAxB,CAAD;EADL,CA9BG;;EAiCbgB,OAAO,GAAG;IACR,KAAKC,WAAL,CAAiB;MACfC,QAAQ,EAAE,EADK;MAEfC,UAAU,EAAE,CAFG;MAGfC,YAAY,EAAE,EAHC;MAIfC,aAAa,EAAE;IAJA,CAAjB;IAMA,KAAKC,eAAL,GAPQ,CAQR;IACA;IACA;EACD,CA5CY;;EA6CbC,KAAK,EAAE;IACL;;;;IAIAhB,UAAU,EAAE,UAAUiB,KAAV,EAAiB;MAC3B,IAAI;QACF,KAAKP,WAAL,CAAiB;UACfC,QAAQ,EAAE,KAAKO,UAAL,CAAgBP,QADX;UAEfC,UAAU,EAAE,CAFG;UAGfC,YAAY,EAAEI,KAHC;UAIfH,aAAa,EAAE,KAAKb;QAJL,CAAjB;MAMF,CAPA,CAOE,OAAOkB,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACF;IACD;EAhBI,CA7CM;EA+DbG,OAAO,EAAE,EACP,GAAG/B,YAAY,CAAC,CAAC,eAAD,EAAkB,aAAlB,EAAiC,aAAjC,CAAD,CADR;IAEP,GAAGC,UAAU,CAAC,CAAC,aAAD,EAAgB,iBAAhB,EAAmC,iBAAnC,CAAD,CAFN;;IAGP;;;;IAIA+B,cAAc,CAACZ,QAAD,EAAWC,UAAX,EAAuB;MACnC,IAAI;QACF,KAAKF,WAAL,CAAiB;UACfC,QAAQ,EAAEA,QADK;UAEfC,UAAU,EAAEA,UAFG;UAGfC,YAAY,EAAE,KAAKb,UAHJ;UAIfc,aAAa,EAAE,KAAKb;QAJL,CAAjB;QAMA,KAAKE,cAAL,GAAsB,IAAtB;MACF,CARA,CAQE,OAAOgB,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACF;IACD,CAnBM;;IAoBP;;;;IAIAK,QAAQ,GAAG;MACT,IAAI;QACF,KAAKd,WAAL,CAAiB;UACfC,QAAQ,EAAE,KAAKO,UAAL,CAAgBP,QADX;UAEfC,UAAU,EAAE,KAAKM,UAAL,CAAgBN,UAFb;UAGfC,YAAY,EAAE,KAAKb,UAHJ;UAIfc,aAAa,EAAE,KAAKb;QAJL,CAAjB;QAMA,KAAKE,cAAL,