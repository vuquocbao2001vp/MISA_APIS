r.push(this.userOfGroupSelected[i].UserID);\n        }\n      }\n\n      console.log(this.groupSelectedId);\n      console.log(this.listDeleteUser);\n\n      try {\n        await axios.delete(`https://localhost:44382/api/v1/UserGroups/DeleteMultiUserOfGroup?groupId=` + this.groupSelectedId, {\n          data: this.listDeleteUser\n        }).then(() => {\n          this.checkUserToDelete = [];\n          this.listDeleteUser = [];\n          this.hideDialog();\n          this.$emit(\"loadData\");\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmEA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,YAAT,EAAuBC,UAAvB,EAAmCC,UAAnC,QAAqD,MAArD;AACA,OAAOC,YAAP,MAAyB,oBAAzB;AACA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;EAIbE,KAAK,EAAE,CAAC,QAAD,EAAW,iBAAX,CAJM;;EAKbC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,IADR;MAELC,OAAO,EAAE,IAFJ;MAGLC,UAAU,EAAE,EAHP;MAILC,iBAAiB,EAAE,EAJd;MAKLC,cAAc,EAAE;IALX,CAAP;EAOD,CAbY;;EAcbC,QAAQ,EAAE,EACR,GAAGV,UAAU,CAAC,CAAC,eAAD,EAAkB,qBAAlB,CAAD;EADL,CAdG;EAiBbW,KAAK,EAAE;IACLC,eAAe,EAAE,UAAUC,KAAV,EAAiB;MAChC,IAAIA,KAAI,IAAK,IAAb,EAAmB;QACjB,KAAKC,gBAAL,CAAsBD,KAAtB;MACF;IACD,CALI;IAMLN,UAAU,EAAE,UAAUM,KAAV,EAAiB;MAC3B,KAAKE,wBAAL,CAA8B;QAC5BC,EAAE,EAAE,EADwB;QAE5BC,SAAS,EAAEJ;MAFiB,CAA9B;IAID,CAXI;IAYLL,iBAAiB,EAAEU;EAZd,CAjBM;EA+BbC,OAAO,EAAE,EACP,GAAGrB,YAAY,CAAC,CAAC,cAAD,EAAiB,sBAAjB,CAAD,CADR;IAEP,GAAGC,UAAU,CAAC,CAAC,kBAAD,EAAqB,0BAArB,CAAD,CAFN;;IAGP;;;;IAIAqB,aAAa,CAAChB,IAAD,EAAO;MAClBiB,YAAY,CAAC,KAAKf,OAAN,CAAZ;MACA,IAAIgB,IAAG,GAAI,IAAX;MACA,KAAKhB,OAAL,GAAeiB,UAAU,CAAC,YAAY;QACpCD,IAAI,CAACf,UAAL,GAAkBH,IAAI,CAACS,KAAvB;MACD,CAFwB,EAEtB,GAFsB,CAAzB;IAGD,CAbM;;IAcP;;;;IAIAW,UAAU,GAAG;MACX,KAAKC,KAAL,CAAW,YAAX,EAAyB,IAAzB;IACD,CApBM;;IAqBP;;;;IAIAC,UAAU,GAAG;MACX,KAAKC,SAAL,CAAe,KAAf;IACD,CA3BM;;IA4BP;;;;IAIAA,SAAS,CAACC,MAAD,EAAS;MAChB,KAAKvB,WAAL,GAAmBuB,MAAnB;IACD,CAlCM;;IAmCP,MAAMC,sBAAN,GAA8B;MAC5B,KAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAI,KAAKC,mBAAL,CAAyBC,MAA7C,EAAqDF,CAAC,EAAtD