 = response.data.Data;\n          this.totalRecords = response.data.TotalRecords;\n          this.totalPages = response.data.TotalPages;\n          this.recordStart = response.data.RecordStart;\n          this.recordEnd = response.data.RecordEnd;\n          this.pageSize = response.data.PageSize;\n          this.pageNumber = response.data.PageNumber;\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Tắt popup khi click vào icon tắt\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    closePopup() {\n      this.$emit(\"hidePopup\", true);\n    },\n\n    /**\r\n     * Lấy ra từ khóa tìm kiếm sau khi nhập xong input\r\n     * Author; VQBao - 8/9/2022\r\n     */\n    getTextSearch(data) {\n      clearTimeout(this.timeout);\n      var self = this;\n      this.timeout = setTimeout(function () {\n        self.textSearch = data.value;\n      }, 500);\n    },\n\n    /**\r\n     * Phương thức thực hiện phân trang người dùng\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    loadMemberPaging(pageSize, pageNumber) {\n      try {\n        this.getUserDataOfGroupPaging(pageSize, pageNumber, '', this.textSearch, this.listJob, this.groupSelected.MISACode);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Khi cây đơn vị thay đổi giá trị, ô input nhận giá trị đó\r\n     */\n    syncTreeViewSelection() {},\n\n    /**\r\n     * Khi phần tử được chọn của cây đơn vị thay đổi, tìm kiếm và phân trang theo giá trị đó\r\n     * Author: VQBao - 15/9/2022\r\n     */\n    treeView_itemSelectionChanged(e) {\n      this.treeBoxValue = e.component.getSelectedNodeKeys();\n      var a = e.component.getSelectedNodes();\n      var code = a[0].itemData.MISACode;\n\n      try {\n        this.getUserDataOfGroupPaging(this.pageSize, 1, '', this.textSearch, this.listJob, a[0].itemData.MISACode);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Khi click chọn 1 phần tử trong cây đơn vị, sau khi lấy ra giá trị thì đóng cây đơn vị\r\n     * Author: VQBao - 15/9/2022\r\n     */\n    onTreeItemClick() {\n      this.isTreeBoxOpened = false;\n      this.treeBoxValue = \"\";\n    },\n\n    /**\r\n     * Tìm kiếm theo chức vụ\r\n     * Author: VQBao - 15/9/2022\r\n     */\n    getJobSearch() {\n      this.listJob = this.listJobPositionSearch.join(',');\n\n      try {\n        this.getUserDataOfGroupPaging(this.pageSize, 1, '', this.textSearch, this.listJob, this.groupSelected.MISACode);\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,YAAT,EAAuBC,UAAvB,EAAmCC,UAAnC,QAAqD,MAArD;AACA,eAAe;EACbC,KAAK,EAAE,CAAC,QAAD,EAAU,eAAV,CADM;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IADJ;MAELC,UAAU,EAAE,EAFP;MAGLC,qBAAqB,EAAE,EAHlB;MAILC,OAAO,EAAE,EAJJ;MAKLC,mBAAmB,EAAE,EALhB;MAMLC,YAAY,EAAE,IANT;MAOLC,UAAU,EAAE,IAPP;MAQLC,QAAQ,EAAE,IARL;MASLC,UAAU,EAAE,IATP;MAULC,WAAW,EAAE,IAVR;MAWLC,SAAS,EAAE,IAXN;MAYLC,eAAe,EAAE,WAZZ;MAaLC,YAAY,EAAE,IAbT;MAcLC,eAAe,EAAE,KAdZ;MAeLC,eAAe,EAAE,CAAC,iBAAD,EAAoB,GAApB,EAAyB,SAAzB;IAfZ,CAAP;EAiBD,CApBY;;EAqBbC,QAAQ,EAAE,EACR,GAAGlB,UAAU,CAAC,CAAC,aAAD,EAAgB,kBAAhB,CAAD;EADL,CArBG;;EAwBbmB,OAAO,GAAG;IACR,KAAKC,kBAAL;IACA,KAAKC,uBAAL;EACD,CA3BY;;EA4BbC,KAAK,EAAE;IACLC,MAAM,EAAE,UAASC,KAAT,EAAe;MACrB,IAAG,CAACA,KAAJ,EAAU;QACR,KAAKC,wBAAL,CAA8B,EAA9B,EAAkC,CAAlC;MACF;IACD,CALI;;IAML;;;;IAIArB,UAAU,EAAE,UAAUoB,KAAV,EAAiB;MAC3B,IAAI;QACF,KAAKC,wBAAL,CAA8B,KAAKf,QAAnC,EAA6C,CAA7C,EAAgD,EAAhD,EAAoDc,KAApD,EAA2D,KAAKlB,OAAhE,EAAyE,KAAKoB,aAAL,CAAmBC,QAA5F;MACF,CAFA,CAEE,OAAOC,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACF;IACD;EAhBI,CA5BM;EA8CbG,OAAO,EAAE,EACP,GAAGjC,YAAY,CAAC,CACd,gBADc,EAEd,qBAFc,CAAD,CADR;IAKP,GAAGC,UAAU,CAAC,CACZ,oBADY,EAEZ,yBAFY,CAAD,CALN;;IASP0B,wBAAwB,CAACf,QAAD,EAAWC,UAAX,EAAuBqB,WAAvB,EAAoCC,YAApC,EAAkDC,gBAAlD,EAAoEP,QAApE,EAA6E;MACnG,IAAI;QACH9B,KAAI,CACFsC,GADF,CACO,+DADP,EACuE;UACpEC,MAAM,EAAE;YACN