{"ast":null,"code":"import DialogUserDetail from \"./DialogUserDetail.vue\";\nimport { mapMutations, mapActions, mapGetters } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    DialogUserDetail\n  },\n\n  data() {\n    return {\n      isShowDialog: false,\n      isHideReorderPopup: true,\n      isHidePopup: true,\n      textSearch: \"\",\n      groupItemSearch: \"\",\n      timeout: null,\n      userSelectedId: null,\n      userIdToDelete: null,\n      isHideToast: true,\n      groups: [],\n      userGridId: \"1c142e9d-5b5b-7f1f-6329-a2925c0f334d\",\n      listGrid: []\n    };\n  },\n\n  computed: { ...mapGetters([\"users\", \"userPaging\", \"userGroups\"]),\n    userGrid: {\n      get() {\n        return this.$store.getters.userGrid;\n      },\n\n      set(value) {\n        this.$store.commit(\"getUserGrid\", value);\n      }\n\n    }\n  },\n\n  mounted() {\n    this.getUserData({\n      pageSize: 50,\n      pageNumber: 1,\n      searchByText: \"\",\n      searchByGroup: \"\"\n    });\n    this.getAllGroupData();\n    this.getUserGridData(this.userGridId); // this.userGroups.unshift({'UserGroupID': 100, 'UserGroupName': \"Tất cả\"})\n    // this.groups = this.userGroups.unshift({UserGroupID: 100, UserGroupName: \"Tất cả\"});\n    // console.log(this.groups);\n  },\n\n  watch: {\n    isHideReorderPopup: function (value) {\n      if (!value && this.listGrid.length == 0) {\n        this.listGrid = this.userGrid.userOption;\n        console.log(\"a\", this.listGrid);\n      }\n    },\n\n    /**\r\n     * Khi từ khóa tìm kiếm thay đổi, load lại paging\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    textSearch: function (value) {\n      try {\n        this.getUserData({\n          pageSize: this.userPaging.pageSize,\n          pageNumber: 1,\n          searchByText: value,\n          searchByGroup: this.groupItemSearch\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  },\n  methods: {\n    onDragEnd(e) {\n      console.log(e, e.fromIndex, e.toIndex);\n      this.listGrid.splice(e.toIndex, 0, this.listGrid.splice(e.fromIndex, 1)[0]);\n      console.log(\"a\", this.listGrid);\n    },\n\n    async saveUserOption() {\n      try {\n        await axios.put(`https://localhost:44382/api/v1/Users/SaveUserOption/` + this.userGridId, this.listGrid).then(() => {\n          this.isHideReorderPopup = true;\n          this.getUserGridData(this.userGridId);\n          this.loadData();\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    saveDefaultOption() {\n      this.listGrid = this.userGrid.defaultOption;\n      this.saveUserOption();\n    },\n\n    ...mapMutations([\"getUserPaging\", \"getUserById\", \"getAllGroup\", \"getUserGrid\"]),\n    ...mapActions([\"getUserData\", \"getUserDataById\", \"getAllGroupData\", \"getUserGridData\"]),\n\n    /**\r\n     * Phương thức thực hiện phân trang người dùng\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    loadUserPaging(pageSize, pageNumber) {\n      try {\n        this.getUserData({\n          pageSize: pageSize,\n          pageNumber: pageNumber,\n          searchByText: this.textSearch,\n          searchByGroup: this.groupItemSearch\n        });\n        this.userSelectedId = null;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Load lại data đầy đủ thông tin paging khi có thay đổi về dữ liệu\r\n     * Author: VQBao - 10/8/2022\r\n     */\n    loadData() {\n      try {\n        this.getUserData({\n          pageSize: this.userPaging.pageSize,\n          pageNumber: this.userPaging.pageNumber,\n          searchByText: this.textSearch,\n          searchByGroup: this.groupItemSearch\n        });\n        this.userSelectedId = null;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Lấy ra từ khóa tìm kiếm sau khi nhập xong input\r\n     * Author; VQBao - 8/9/2022\r\n     */\n    getTextSearch(data) {\n      clearTimeout(this.timeout);\n      var self = this;\n      this.timeout = setTimeout(function () {\n        self.textSearch = data.value;\n      }, 500);\n    },\n\n    /**\r\n     * Khi chọn nhóm người dùng, phân trang người dùng lọc theo nhóm người dùng đó\r\n     * Author: VQBao - 8/9/2022\r\n     */\n    getGroupItemSearch(data) {\n      this.groupItemSearch = data.value;\n\n      try {\n        this.getUserData({\n          pageSize: this.userPaging.pageSize,\n          pageNumber: 1,\n          searchByText: this.textSearch,\n          searchByGroup: data.value\n        });\n        this.userSelectedId = null;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Khi click vào hàng trong bảng thì cửa sổ thông tin chi tiết hiện ra\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    rowOnClick(userRow) {\n      this.userSelectedId = userRow.data.UserID;\n      this.showDialog(true);\n    },\n\n    /**\r\n     * Ẩn hiện cửa sổ thông tin chi tiết người dùng\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    showDialog(isShow) {\n      this.isShowDialog = isShow;\n    },\n\n    /**\r\n     * Ẩn hiện popup thông báo\r\n     * Author: VQBao - 10/8/2022\r\n     */\n    hidePopup(isHide) {\n      this.isHidePopup = isHide;\n    },\n\n    /**\r\n     * Hiện toast message\r\n     * Author: VQBao - 10/8/2022\r\n     */\n    hideToast() {\n      var me = this;\n      me.isHideToast = false;\n      setTimeout(function () {\n        me.isHideToast = true;\n      }, 3000);\n    },\n\n    /**\r\n     * Xóa người dùng khi bấm vào button xóa ở cuối dòng trong bảng\r\n     * Author: VQBao - 10/9/2022\r\n     */\n    deleteUserOnClick(e) {\n      this.userIdToDelete = e.data.UserID;\n      this.hidePopup(false);\n    },\n\n    /**\r\n     * Xóa người dùng(dùng để emit sau confirm xóa)\r\n     * Author: VQBao - 10/9/2022\r\n     */\n    deleteUser() {\n      try {\n        axios.delete(`https://localhost:44382/api/v1/Users/` + this.userIdToDelete).then(() => {\n          // load lại data\n          this.getUserData({\n            pageSize: this.userPaging.pageSize,\n            pageNumber: 1,\n            searchByText: this.textSearch,\n            searchByGroup: this.groupItemSearch\n          });\n          this.hideToast();\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * Ẩn hiện cửa sổ popup tùy chỉnh cột\r\n     * Author: VQBao - 30/8/2022\r\n     */\n    reorderColumn() {\n      this.isHideReorderPopup = !this.isHideReorderPopup;\n    },\n\n    /**\r\n     * Xuất khẩu toàn bộ người dùng ra file excel\r\n     * Author: VQBao - 12/9/2022\r\n     */\n    exportToExcel() {\n      try {\n        axios({\n          url: \"https://localhost:44382/api/v1/Users/ExportToExcel\",\n          method: \"GET\",\n          responseType: \"blob\"\n        }).then(response => {\n          var fileURL = window.URL.createObjectURL(new Blob([response.data]));\n          var fileLink = document.createElement(\"a\");\n          fileLink.href = fileURL;\n          fileLink.setAttribute(\"download\", \"Danh_sach_nguoi_dung.xlsx\");\n          document.body.appendChild(fileLink);\n          fileLink.click();\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsIA,OAAOA,gBAAP,MAA6B,wBAA7B;AACA,SAASC,YAAT,EAAuBC,UAAvB,EAAmCC,UAAnC,QAAqD,MAArD;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAE;IACVL;EADU,CADC;;EAIbM,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,KADT;MAELC,kBAAkB,EAAE,IAFf;MAGLC,WAAW,EAAE,IAHR;MAILC,UAAU,EAAE,EAJP;MAKLC,eAAe,EAAE,EALZ;MAMLC,OAAO,EAAE,IANJ;MAOLC,cAAc,EAAE,IAPX;MAQLC,cAAc,EAAE,IARX;MASLC,WAAW,EAAE,IATR;MAULC,MAAM,EAAE,EAVH;MAWLC,UAAU,EAAE,sCAXP;MAYLC,QAAQ,EAAE;IAZL,CAAP;EAcD,CAnBY;;EAoBbC,QAAQ,EAAE,EACR,GAAGhB,UAAU,CAAC,CAAC,OAAD,EAAU,YAAV,EAAwB,YAAxB,CAAD,CADL;IAERiB,QAAQ,EAAE;MACRC,GAAG,GAAG;QACJ,OAAO,KAAKC,MAAL,CAAYC,OAAZ,CAAoBH,QAA3B;MACD,CAHO;;MAIRI,GAAG,CAACC,KAAD,E